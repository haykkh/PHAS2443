          RowBox[{"-", "0.3`"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0.3`"}], "}"}]}], "}"}], "/.", 
      "\[InvisibleSpace]", "x0"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"l2", "=", 
   RowBox[{"Graphics", "[", 
    RowBox[{"Line", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"-", "0.3`"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0.3`"}], "}"}]}], "}"}], "/.", 
      "\[InvisibleSpace]", "x1"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
