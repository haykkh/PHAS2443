       RowBox[{"\[PartialD]", "x"}]]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["\[CapitalDelta]x", "2"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
