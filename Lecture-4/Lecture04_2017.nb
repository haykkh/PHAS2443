         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "2"]}]]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"3", "!"}]], 
      SuperscriptBox["\[CapitalDelta]x", "3"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "3"], 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "3"]}]]}], "+"}], "..."}], TraditionalForm]]],
 "\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", 
     RowBox[{
      RowBox[{"i", "-", "1"}], ",", "n"}]], "="}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Theta]", 
      RowBox[{"i", ",", "n"}]], "-", " ", 
     RowBox[{"\[CapitalDelta]x", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", "x"}]]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["\[CapitalDelta]x", "2"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "2"]}]]}], "-", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"3", "!"}]], 
      SuperscriptBox["\[CapitalDelta]x", "3"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "3"], 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "3"]}]]}], "+"}], "..."}], TraditionalForm]]],
 "\n so that we obtain\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Theta]", 
      RowBox[{"i", ",", "n"}]], "+", " ", 
     RowBox[{"\[CapitalDelta]t", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", "t"}]]}], "+", 
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox["\[CapitalDelta]t", "2"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["t", "2"]}]]}], "+"}], "..."}], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{"\[Zeta]", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           SubscriptBox["\[Theta]", 
            RowBox[{"i", ",", "n"}]], "+", " ", 
           RowBox[{"\[CapitalDelta]x", " ", 
            FractionBox[
             RowBox[{"\[PartialD]", 
              SubscriptBox["\[Theta]", 
               RowBox[{"i", ",", "n"}]]}], 
             RowBox[{"\[PartialD]", "x"}]]}], "+", 
           RowBox[{
            FractionBox["1", "2"], 
            SuperscriptBox["\[CapitalDelta]x", "2"], 
            FractionBox[
             RowBox[{
              SuperscriptBox["\[PartialD]", "2"], 
              SubscriptBox["\[Theta]", 
               RowBox[{"i", ",", "n"}]]}], 
             RowBox[{"\[PartialD]", 
              SuperscriptBox["x", "2"]}]]}], "+", 
           RowBox[{
            FractionBox["1", 
             RowBox[{"3", "!"}]], 
            SuperscriptBox["\[CapitalDelta]x", "3"], 
            FractionBox[
             RowBox[{
              SuperscriptBox["\[PartialD]", "3"], 
              SubscriptBox["\[Theta]", 
               RowBox[{"i", ",", "n"}]]}], 
             RowBox[{"\[PartialD]", 
              SuperscriptBox["x", "3"]}]]}], "+"}], "..."}], "+", 
         SubscriptBox["\[Theta]", 
          RowBox[{"i", ",", "n"}]], "-", " ", 
         RowBox[{"\[CapitalDelta]x", " ", 
          FractionBox[
           RowBox[{"\[PartialD]", 
            SubscriptBox["\[Theta]", 
             RowBox[{"i", ",", "n"}]]}], 
           RowBox[{"\[PartialD]", "x"}]]}], "+", 
         RowBox[{
          FractionBox["1", "2"], 
          SuperscriptBox["\[CapitalDelta]x", "2"], 
          FractionBox[
           RowBox[{
            SuperscriptBox["\[PartialD]", "2"], 
            SubscriptBox["\[Theta]", 
             RowBox[{"i", ",", "n"}]]}], 
           RowBox[{"\[PartialD]", 
            SuperscriptBox["x", "2"]}]]}], "-", 
         RowBox[{
          FractionBox["1", 
           RowBox[{"3", "!"}]], 
          SuperscriptBox["\[CapitalDelta]x", "3"], 
          FractionBox[
           RowBox[{
            SuperscriptBox["\[PartialD]", "3"], 
            SubscriptBox["\[Theta]", 
             RowBox[{"i", ",", "n"}]]}], 
           RowBox[{"\[PartialD]", 
            SuperscriptBox["x", "3"]}]]}], "+"}], "..."}], ")"}]}], " ", "+", 
     " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"2", "\[Zeta]"}]}], ")"}], " ", 
      SubscriptBox["\[Theta]", 
       RowBox[{"i", ",", "n"}]]}]}]}], TraditionalForm]]],
 ".\nRearranging,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{
      RowBox[{"\[CapitalDelta]t", " ", 
       FractionBox[
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Theta]", 
          RowBox[{"i", ",", "n"}]]}], 
        RowBox[{"\[PartialD]", "t"}]]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["\[CapitalDelta]t", "2"], 
       FractionBox[
        RowBox[{
         SuperscriptBox["\[PartialD]", "2"], 
         SubscriptBox["\[Theta]", 
          RowBox[{"i", ",", "n"}]]}], 
        RowBox[{"\[PartialD]", 
         SuperscriptBox["t", "2"]}]]}], "+"}], "..."}]}], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{"\[Zeta]", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox["\[CapitalDelta]x", "2"], 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "2"], 
           SubscriptBox["\[Theta]", 
            RowBox[{"i", ",", "n"}]]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["x", "2"]}]]}], "+", 
        RowBox[{
         FractionBox["2", 
          RowBox[{"4", "!"}]], 
         SuperscriptBox["\[CapitalDelta]x", "4"], 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "4"], 
           SubscriptBox["\[Theta]", 
            RowBox[{"i", ",", "n"}]]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["x", "4"]}]]}], "+"}], "..."}], ")"}], " "}]}], 
   TraditionalForm]]],
 "\nor (recalling that \[Zeta]=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[Kappa]", " ", "\[CapitalDelta]t"}], 
    RowBox[{"c\[Rho]", " ", 
     SuperscriptBox["\[CapitalDelta]x", "2"]}]], TraditionalForm]]],
 ")\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SubscriptBox["\[Theta]", 
        RowBox[{"i", ",", "n"}]]}], 
      RowBox[{"\[PartialD]", "t"}]], "-", 
     RowBox[{
      FractionBox["\[Kappa]", "c\[Rho]"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "2"]}]]}]}]}], TraditionalForm]]],
 "=",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"2", "\[Zeta]"}], 
         RowBox[{"4", "!"}]], 
        SuperscriptBox["\[CapitalDelta]x", "2"], 
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "4"], 
          SubscriptBox["\[Theta]", 
           RowBox[{"i", ",", "n"}]]}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "4"]}]]}], "-", 
       RowBox[{
        FractionBox["1", "2"], "\[CapitalDelta]t", 
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], 
          SubscriptBox["\[Theta]", 
           RowBox[{"i", ",", "n"}]]}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["t", "2"]}]]}]}], "..."}], "\[IndentingNewLine]", 
     RowBox[{"=", 
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["2", 
          RowBox[{"4", "!"}]], 
         FractionBox["\[Kappa]", "c\[Rho]"], "\[CapitalDelta]t", 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "4"], 
           SubscriptBox["\[Theta]", 
            RowBox[{"i", ",", "n"}]]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["x", "4"]}]]}], "-", 
        RowBox[{
         FractionBox["1", "2"], "\[CapitalDelta]t", 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "2"], 
           SubscriptBox["\[Theta]", 
            RowBox[{"i", ",", "n"}]]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["t", "2"]}]]}]}], "..."}]}]}]}], TraditionalForm]]],
 "\n and we can see that as \[CapitalDelta]t tends to zero the right-hand \
side tends to zero, and we recover\n ",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", 
       SubscriptBox["\[Theta]", 
        RowBox[{"i", ",", "n"}]]}], 
      RowBox[{"\[PartialD]", "t"}]], "-", 
     RowBox[{
      FractionBox["\[Kappa]", "c\[Rho]"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], 
        SubscriptBox["\[Theta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "2"]}]]}]}]}], TraditionalForm]]],
 "=0,\n the original differential equation.\n \nWe can go through this more \
formally, by considering the way in which the finite difference solution \
converges on the exact solution. In this case we need to add one more \
mathematical trick to our armoury. This is the fact that for any \
single-valued function ",
 StyleBox["f",
  FontSlant->"Italic"],
 "(",
 StyleBox["x",
  FontSlant->"Italic"],
 ") which is continuous in the range ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "\[LessSlantEqual]", "x", "\[LessSlantEqual]", 
    SubscriptBox["x", "2"]}], TraditionalForm]]],
 " it is possible to write\n  ",
 StyleBox["f",
  FontSlant->"Italic"],
 "(",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["x", "3"], ")"}], " ", "=", " ", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"f", "(", 
        SubscriptBox["x", "1"], ")"}], "+", 
       RowBox[{"f", "(", 
        SubscriptBox["x", "2"], ")"}]}], ")"}]}]}], TraditionalForm]]],
 " \n  where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "\[LessSlantEqual]", 
    SubscriptBox["x", "3"], "\[LessSlantEqual]", 
    RowBox[{
     SubscriptBox["x", "2"], "."}]}], TraditionalForm]]],
 " The best way of convincing oneself of the truth of this is probably by \
drawing a graph.\n  \n    Now suppose that in solving the diffusion equation \
using the explicit method,\n     ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", 
     RowBox[{"i", ",", 
      RowBox[{"n", "+", "1"}]}]], "=", " ", 
    RowBox[{
     RowBox[{"\[Zeta]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Theta]", 
         RowBox[{
          RowBox[{"i", "+", "1"}], ",", "n"}]], "+", 
        SubscriptBox["\[Theta]", 
         RowBox[{
          RowBox[{"i", "-", "1"}], ",", "n"}]]}], ")"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"2", "\[Zeta]"}]}], ")"}], " ", 
      SubscriptBox["\[Theta]", 
       RowBox[{"i", ",", "n"}]]}]}]}], TraditionalForm]]],
 ",\n     the values \[Theta] found in the numerical simulation differ \
slightly from the exact values, \[CapitalTheta]. Write the error term as\n    \
 ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", 
     RowBox[{"i", ",", "n"}]], "="}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", 
     RowBox[{"i", ",", "n"}]], "-", 
    SubscriptBox["\[CapitalTheta]", 
     RowBox[{"i", ",", "n"}]]}], TraditionalForm]]],
 ".\n     Substituting, then, we find\n     ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", 
     RowBox[{"i", ",", 
      RowBox[{"n", "+", "1"}]}]], "=", " ", 
    RowBox[{
     RowBox[{"\[Zeta]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Epsilon]", 
         RowBox[{
          RowBox[{"i", "+", "1"}], ",", "n"}]], "+", 
        SubscriptBox["\[Epsilon]", 
         RowBox[{
          RowBox[{"i", "-", "1"}], ",", "n"}]]}], ")"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"2", "\[Zeta]"}]}], ")"}], " ", 
      SubscriptBox["\[Epsilon]", 
       RowBox[{"i", ",", "n"}]]}], "+", 
     RowBox[{"\[Zeta]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[CapitalTheta]", 
         RowBox[{
          RowBox[{"i", "+", "1"}], ",", "n"}]], "+", 
        SubscriptBox["\[CapitalTheta]", 
         RowBox[{
          RowBox[{"i", "-", "1"}], ",", "n"}]]}], ")"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"2", "\[Zeta]"}]}], ")"}], " ", 
      SubscriptBox["\[CapitalTheta]", 
       RowBox[{"i", ",", "n"}]]}], "-", 
     SubscriptBox["\[CapitalTheta]", 
      RowBox[{"i", ",", 
       RowBox[{"n", "+", "1"}]}]]}]}], TraditionalForm]]],
 ". \n     Now use the Taylor expansion, but keep the correct remainder term \
(remember that this involved the properties of the function at a point \
somewhere within the expansion interval -- you can see where this is going). \
So, for example,\n       ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalTheta]", 
     RowBox[{
      RowBox[{"i", "+", "1"}], ",", "n"}]], "=", " ", 
    RowBox[{
     SubscriptBox["\[CapitalTheta]", 
      RowBox[{"i", ",", "n"}]], "+", 
     RowBox[{"\[CapitalDelta]x", 
      FractionBox[
       RowBox[{"\[PartialD]", 
        SubscriptBox["\[CapitalTheta]", 
         RowBox[{"i", ",", "n"}]]}], 
       RowBox[{"\[PartialD]", "x"}]]}], " ", "+", 
     RowBox[{
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", "\[CapitalDelta]x", ")"}], "2"], "2"], " ", 
      SubscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "\[CapitalTheta]"}], 
         RowBox[{"\[PartialD]", 
          SuperscriptBox["x", "2"]}]], ")"}], 
       RowBox[{
        RowBox[{"x", "=", 
         OverscriptBox["x", "_"]}], ",", "n"}]]}]}]}], TraditionalForm]]],
 ",\n       where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", " ", "\[CapitalDelta]x"}], "\[LessSlantEqual]", 
    OverscriptBox["x", "_"], "\[LessSlantEqual]", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"i", "+", "1"}], ")"}], 
     RowBox[{"\[CapitalDelta]x", "."}]}]}], TraditionalForm]]],
 "\n       We can do the same thing for the expansion towards (",
 StyleBox["i",
  FontSlant->"Italic"],
 "-1)\[CapitalDelta]x and for the expansion in time, and put the results \
together, to find\n       ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", 
     RowBox[{"i", ",", 
      RowBox[{"n", "+", "1"}]}]], "=", " ", 
    RowBox[{
     RowBox[{"\[Zeta]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Epsilon]", 
         RowBox[{
          RowBox[{"i", "+", "1"}], ",", "n"}]], "+", 
        SubscriptBox["\[Epsilon]", 
         RowBox[{
          RowBox[{"i", "-", "1"}], ",", "n"}]]}], ")"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{"2", "\[Zeta]"}]}], ")"}], " ", 
      SubscriptBox["\[Epsilon]", 
       RowBox[{"i", ",", "n"}]]}], "-", 
     RowBox[{"\[CapitalDelta]t", " ", "[", 
      RowBox[{
       SubscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{"\[PartialD]", "\[CapitalTheta]"}], 
          RowBox[{"\[PartialD]", "t"}]], ")"}], 
        RowBox[{"i", ",", 
         RowBox[{"t", "=", 
          OverscriptBox["t", "_"]}]}]], "-", 
       RowBox[{
        FractionBox["\[Kappa]", 
         RowBox[{"2", "c\[Rho]"}]], 
        RowBox[{"{", " ", 
         RowBox[{
          SubscriptBox[
           RowBox[{"(", 
            FractionBox[
             RowBox[{
              SuperscriptBox["\[PartialD]", "2"], "\[CapitalTheta]"}], 
             RowBox[{"\[PartialD]", 
              SuperscriptBox["x", "2"]}]], ")"}], 
           RowBox[{
            RowBox[{"x", "=", 
             OverscriptBox["x", "_"]}], ",", "n"}]], "+", " ", 
          SubscriptBox[
           RowBox[{"(", 
            FractionBox[
             RowBox[{
              SuperscriptBox["\[PartialD]", "2"], "\[CapitalTheta]"}], 
             RowBox[{"\[PartialD]", 
              SuperscriptBox["x", "2"]}]], ")"}], 
           RowBox[{
            RowBox[{"x", "=", 
             OverscriptBox[
              RowBox[{"x", "'"}], "_"]}], ",", "n"}]]}], "}"}]}]}], "]"}]}]}],
    TraditionalForm]]],
 ",\n       where ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["x", "_"], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   OverscriptBox[
    RowBox[{"x", "'"}], "_"], TraditionalForm]]],
 " represent the error terms in the forward and backward differences in \
space. Now at any time step, ",
 StyleBox["n",
  FontSlant->"Italic"],
 ", there will be different error terms for different space points ",
 StyleBox["i",
  FontSlant->"Italic"],
 ". As long as we insist that (1-2\[Zeta]) is positive or zero, though, we \
can work in terms of the maximum error at any step, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "n"], TraditionalForm]]],
 ", and if we write the quantity in square brackets as ",
 StyleBox["Q",
  FontSlant->"Italic"],
 " we have\n       |",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Epsilon]", 
     RowBox[{"i", ",", 
      RowBox[{"n", "+", "1"}]}]], "|"}], TraditionalForm]]],
 " \[LessSlantEqual] \[Zeta](",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["E", "n"], "+", 
      SubscriptBox["E", "n"]}], ")"}], " ", "+", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[Zeta]"}]}], ")"}], " ", 
     SubscriptBox["E", "n"]}], " ", "+", " ", 
    RowBox[{"Q", " ", 
     StyleBox[
      RowBox[{"\[CapitalDelta]", 
       StyleBox["t",
        FontSlant->"Italic"]}]]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", " ", 
    RowBox[{
     SubscriptBox["E", "n"], " ", "+", " ", 
     RowBox[{"Q", " ", 
      RowBox[{
       StyleBox[
        RowBox[{"\[CapitalDelta]", 
         StyleBox["t",
          FontSlant->"Italic"]}]], "."}]}]}]}], TraditionalForm]}]],
 "\n       So we can see that as the time-step \[CapitalDelta]",
 StyleBox["t",
  FontSlant->"Italic"],
 " tends to zero the growth in the error also tends to zero -- but as the \
error in the initial condition is assumed to be zero this means that the \
error tends to zero. Furthermore, as both \[CapitalDelta]",
 StyleBox["x",
  FontSlant->"Italic"],
 " and \[CapitalDelta]",
 StyleBox["t ",
  FontSlant->"Italic"],
 "tend to zero the term in square brackets tends to the differential equation\
\n       ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[CapitalTheta]"}], 
      RowBox[{"\[PartialD]", "t"}]], "-", 
     RowBox[{
      FractionBox["\[Kappa]", "c\[Rho]"], 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[PartialD]", "2"], "\[CapitalTheta]"}], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["x", "2"]}]]}]}], "]"}], TraditionalForm]]],
 "\n   with all the terms evaluated at the same ",
 StyleBox["x",
  FontSlant->"Italic"],
 " and ",
 StyleBox["t, ",
  FontSlant->"Italic"],
 StyleBox["which we know is zero. ",
  FontVariations->{"CompatibilityType"->0}],
 "\n       \n        We have proved that the difference equation we have \
derived can be made consistent. The condition for consistency is rather \
boring, as it is the same as the stability condition, but this is not always \
the case. The exercises accompanying this lecture explore a more interesting \
case.  "
}], "Text",
 CellChangeTimes->{{3.6318628799975986`*^9, 3.6318628843461657`*^9}, {
