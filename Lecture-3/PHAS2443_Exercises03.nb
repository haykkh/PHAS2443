Cell[TextData[{
 StyleBox["1.",
  FontColor->RGBColor[1, 0, 0]],
 StyleBox[" Write a generalised versions of the explicit Euler scheme, which \
will work for any function f(t,y) on the right-hand side of \n",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  StyleBox[
   RowBox[{
    FractionBox[
     RowBox[{
      StyleBox[
       RowBox[{"d", 
        StyleBox["y",
         FontSlant->"Italic"]}]], 
      StyleBox[
       RowBox[{"(", "t", ")"}],
       FontSlant->"Italic"]}], "dt"], "=", 
    StyleBox[" ",
     FontSlant->"Italic"], 
    StyleBox[
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"t", ",", "y"}], ")"}]}],
     FontSlant->"Italic"]}],
   FontFamily->"Times New Roman"]],
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "42e8cbc8-77fa-4b2b-9a26-bc0f918f8c4e"],
 StyleBox["\n(the function you write should take ",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["f",
  FontSlant->"Italic",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox[" as an argument), and use it to solve\n",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    StyleBox[
     FractionBox[
      RowBox[{
       StyleBox[
        RowBox[{"d", 
         StyleBox["y",
          FontSlant->"Italic"]}]], 
       StyleBox[
        RowBox[{"(", "t", ")"}],
        FontSlant->"Italic"]}], "dt"],
     FontFamily->"Times New Roman"], 
    StyleBox["=",
     FontFamily->"Times New Roman"], 
    StyleBox[" ",
     FontFamily->"Times New Roman",
     FontSlant->"Italic"], 
    RowBox[{
     StyleBox["t",
      FontFamily->"Times New Roman",
      FontSlant->"Italic"], 
     StyleBox[" ",
      FontFamily->"Times New Roman",
      FontSlant->"Italic"], 
     StyleBox["y",
      FontFamily->"Times New Roman",
      FontSlant->"Italic"], 
     SuperscriptBox[
      StyleBox[
       RowBox[{"(", "t", ")"}],
       FontFamily->"Times New Roman",
       FontSlant->"Italic"], "2"]}]}], "\[IndentingNewLine]"}]],
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "538f7640-faaf-4309-b61d-5d8b2688c1a6"],
 StyleBox["\nwith ",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["y",
  FontSlant->"Italic",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["(0)= - 1 over the range 0\[LessSlantEqual]",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["t",
  FontSlant->"Italic",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["\[LessSlantEqual]10. Use timesteps of 1.0, 0.5 and 0.1, and plot \
your results for each case with the exact solution for comparison. This shows \
that the Euler method is capable of coping, in this case at least, with a \
nonlinear problem.",
  FontColor->RGBColor[0, 0, 1]]
}], "Text",ExpressionUUID->"f21afaaa-0f36-4813-912e-f23050cf56cf"],

