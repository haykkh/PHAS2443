 RowBox[{
  RowBox[{"mmatinvnmat", "=", 
   RowBox[{"mmatinv", ".", "nmat"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"mmatinvbvec", "=", 
   RowBox[{"mmatinv", ".", "bvec"}]}], ";"}], "\n", 
 RowBox[{
