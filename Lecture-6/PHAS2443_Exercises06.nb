Cell[TextData[{
 StyleBox["2.",
  FontColor->RGBColor[1, 0, 0]],
 StyleBox[" Below is the code used in  Lecture 6 to model the scattering of a \
quantum wave-packet from a potential barrier.  This barrier is defined to \
have a smooth shape, and the incident pulse is a gaussian-modulated complex \
exponential. Set up and solve the same problem using ",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["NDSolve",
  FontFamily->"Courier New",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox[", and compare the results of the calculations and the times \
taken.",
  FontColor->RGBColor[0, 0, 1]]
}], "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[CapitalDelta]t", "=", "0.15`"}], ";", 
  RowBox[{"\[CapitalDelta]x", "=", "0.5`"}], ";", 
  RowBox[{"\[Zeta]", "=", 
   FractionBox["\[CapitalDelta]t", 
    SuperscriptBox["\[CapitalDelta]x", "2"]]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"x0", "=", 
   RowBox[{"-", "50"}]}], ";", 
  RowBox[{"xmax", "=", "100"}], ";", 
  RowBox[{"w", "=", "20"}], ";", 
  RowBox[{"V0", "=", "1.`"}], ";", 
  RowBox[{"a", "=", "5.`"}], ";", 
  RowBox[{"k", "=", "1"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\[Psi]", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", 
        FractionBox[
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"x", "-", "x0"}], ")"}], "2"], 
         SuperscriptBox["a", "3"]]}]], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", "k", " ", "x"}]]}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "xmax"}], ",", "xmax", ",", "\[CapitalDelta]x"}], "}"}]}],
     "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"V", "[", "x_", "]"}], ":=", 
   RowBox[{
    FractionBox["1", "4"], " ", "V0", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Tanh", "[", 
       RowBox[{"x", "+", 
        FractionBox["w", "2"]}], "]"}], "-", 
      RowBox[{"Tanh", "[", 
       RowBox[{"x", "-", 
        FractionBox["w", "2"]}], "]"}]}], ")"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"v", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"V", "[", "x", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "xmax"}], ",", "xmax", ",", "\[CapitalDelta]x"}], "}"}]}],
     "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"bcl", "=", "0"}], ";", 
  RowBox[{"bcr", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"nx", "=", 
   RowBox[{
    RowBox[{"Floor", "[", 
     FractionBox[
      RowBox[{"2", " ", "xmax"}], "\[CapitalDelta]x"], "]"}], "+", "1"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"bvec", "=", 
   RowBox[{"2", " ", "\[ImaginaryI]", " ", "\[Zeta]", " ", 
    RowBox[{"Append", "[", 
     RowBox[{
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "2", ",", 
            RowBox[{"nx", "-", "1"}]}], "}"}]}], "]"}], ",", "bcl"}], "]"}], 
      ",", "bcr"}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"mmat", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Which", "[", 
      RowBox[{
       RowBox[{"i", "\[Equal]", "j"}], ",", 
       RowBox[{
        RowBox[{"2", " ", 
