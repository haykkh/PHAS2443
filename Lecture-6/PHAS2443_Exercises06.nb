    RowBox[{
     RowBox[{"Which", "[", 
      RowBox[{
       RowBox[{"i", "\[Equal]", "j"}], ",", 
       RowBox[{
        RowBox[{"2", " ", 
         RowBox[{"(", 
          RowBox[{"2", "-", 
           RowBox[{"\[ImaginaryI]", " ", "\[Zeta]"}]}], ")"}]}], "-", 
        RowBox[{"2", " ", "\[ImaginaryI]", " ", "\[CapitalDelta]t", " ", 
         RowBox[{
         "v", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}]}], ",",
        
       RowBox[{"i", "\[Equal]", 
